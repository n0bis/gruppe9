<AnchorPane id="AnchorPane" fx:id="rootId" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MenuController">

   <children>
      <ImageView fx:id="menubarBackground" fitHeight="108.0" fitWidth="1003.0" pickOnBounds="true" preserveRatio="true">
