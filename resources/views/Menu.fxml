      <ImageView id="menuSpellbook" fx:id="spellBookId" fitHeight="94.0" fitWidth="126.0" layoutX="226.0" layoutY="7.0" onMouseClicked="#spellBookClicked" pickOnBounds="true" preserveRatio="true" />
